// paste this into your console when you're on tradingview.com
let items = [document.querySelector("#overlap-manager-root > div:nth-child(2) > div > div > div"),
    document.querySelector("#overlap-manager-root > div:nth-child(2) > div > div")
]
for (var i = 0; i < items.length; i++) {
    items[i].parentNode.removeChild(items[i]);
}